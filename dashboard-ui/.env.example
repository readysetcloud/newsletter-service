# Frontend Environment Variables Example
# Copy this file to .env for local development and fill in actual values

# Cognito Configuration
# Get these values from AWS Console > Cognito or CloudFormation outputs
VITE_USER_POOL_ID=us-east-1_XXXXXXXXX
VITE_USER_POOL_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXXX
VITE_IDENTITY_POOL_ID=us-east-1:XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
VITE_AWS_REGION=us-east-1

# API Configuration
# Get this from CloudFormation outputs: DashboardApiUrl
VITE_API_BASE_URL=https://XXXXXXXXXX.execute-api.us-east-1.amazonaws.com/api

# Application Settings (optional - defaults are provided)
# VITE_APP_NAME=Newsletter Dashboard
# VITE_APP_VERSION=1.0.0
# VITE_API_TIMEOUT=10000

# Feature Flags (optional - defaults are provided)
# VITE_ENABLE_ANALYTICS=true
# VITE_ENABLE_ERROR_REPORTING=true
# VITE_ENABLE_PERFORMANCE_MONITORING=true
# VITE_ENABLE_DEBUG_MODE=false

# Performance Settings (optional - defaults are provided)
# VITE_BUNDLE_ANALYZER=false
# VITE_PRELOAD_ROUTES=true
# VITE_LAZY_LOAD_IMAGES=true

# Security Settings (optional - defaults are provided)
# VITE_ENABLE_CSP=true
# VITE_ENABLE_HTTPS_ONLY=true
